{
  "name": "nextjs-dashboard-tut",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "workspace",
  "remoteEnv": {
    "LOCAL_WORKSPACE_FOLDER": "${localWorkspaceFolder}"
  },
  // "remoteUser": "vscode",
  "workspaceFolder": "/nextjs-dashboard-tut",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "EditorConfig.EditorConfig",
        "shardulm94.trailing-spaces"
      ]
    }
  },
  "features": {
    "docker-from-docker": {
      "version": "latest",
      "dockerDashComposeVersion": "v2"
    }
  }
}
